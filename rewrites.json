[
  {
    "from": "humans.txt",
    "to": "assets/humans.txt"
  },
  {
    "from": "robots.txt",
    "to": "assets/robots.txt"
  },
  {
    "from": "crossdomain.xml",
    "to": "assets/crossdomain.xml"
  },
  {
    "from": "apple-touch-icon.png",
    "to": "assets/img/apple-touch-icon.png"
  },
  {
    "from": "favicon.ico",
    "to": "assets/img/favicon.ico"
  },
  {
    "from": "/img/*",
    "to": "assets/img/*"
  },
  {
    "from": "/images/*", // Foundation 3 (foundation.zurb.com) img assets
    "to": "assets/img/*"
  },
  {
    "from": "/css/*",
    "to": "assets/css/*"
  },
  {
    "from": "/js/*",
    "to": "assets/js/*"
  },
  {
    "from": "/fonts/*",
    "to": "assets/fonts/*"
  },
  {
    "from": "",
    "to": "_list/layout/timeline",
    "query": {
      "layout": "layout",
      "template": "index",
      "include_docs": "true",
      "update_seq": "true",
      "descending": "true",
      "limit": "100"
    }
  },
  {
    "from": "/blog/",
    "to": "_list/layout/blog",
    "query": {
      "layout": "layout",
      "template": "blog",
      "include_docs": "true",
      "update_seq": "true",
      "descending": "true",
      "limit": "100"
    }
  },
  {
    "from": "/feed.xml",
    "to": "_list/rssfeed/blog",
    "query": {
      "layout": "rssfeed",
      "template": "rssitems",
      "include_docs": "true",
      "descending": "true",
      "limit": "100"
     }
  },
  {
    "from": "/blog/:id",
    "to": "_show/layout/:id",
    "query": {
      "layout": "layout",
      "template": "post"
    }
  },
  {
    "from": ":id",
    "to": "_show/layout/:id",
    "query": {
      "layout": "layout",
      "template": "page"
    }
  },
  {
    "from": "api/ddoc/*",
    "to": "*"
  },
  {
    "from": "api/*",
    "to": "../../*"
  }
]
