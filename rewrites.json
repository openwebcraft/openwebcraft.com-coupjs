[
  {
    "from": "app.js",
    "to": "/js/app.js"
  },
  {
    "from": "*.png",
    "to": "*.png"
  },
  {
    "from": "/css/*",
    "to": "assets/css/*"
  },
  {
    "from": "/js/*",
    "to": "assets/js/*"
  },
  {
    "from": "",
    "to": "_list/layout/timeline",
    "query": {
      "template": "home",
      "include_docs": "true",
      "update_seq": "true",
      "descending": "true",
      "limit": "100"
    }
  },
  {
    "from": ":id",
    "to": "_show/layout/:id",
    "query": {
      "template": "page"
    }
  },
  {
    "from": "api/ddoc/*",
    "to": "*"
  },
  {
    "from": "api/*",
    "to": "../../*"
  }
]
