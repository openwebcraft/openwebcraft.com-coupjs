{
  "docs": [
    {
      "_id": "_design/pages",

      "lib": <%= map 'lib' %>,
      "models": <%= map 'models' %>,
      "templates": <%= map 'templates' %>,

      "views": <%= map 'views' %>,
      "lists": <%= map 'lists' %>,
      "shows": <%= map 'shows' %>,

      "rewrites": <%= read 'rewrites.json' %>,
      
      "_attachments": <%= attachments ["assets/humans.txt", "assets/crossdomain.xml", "assets/robots.txt", "assets/apple-touch-icon.png", "assets/favicon.ico", "assets/img/**/*.*", "assets/css/**/*.*", "assets/js/**/*.*"] %>
    },
    <%= read 'pages/about.json' %>,
    <%= read 'pages/imprint.json' %>
  ]
}
