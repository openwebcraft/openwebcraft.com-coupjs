{
  "docs": [
    {
      "_id": "_design/pages",

      "lib": <%= map 'lib' %>,
      "models": <%= map 'models' %>,
      "templates": <%= map 'templates' %>,

      "views": <%= map 'views' %>,
      "lists": <%= map 'lists' %>,
      "shows": <%= map 'shows' %>,

      "rewrites": <%= read 'rewrites.json' %>,

      "validate_doc_update": <%= h read 'validate_doc_update.js' %>,
      
      "_attachments": <%= attachments ["assets/humans.txt", "assets/crossdomain.xml", "assets/robots.txt", "assets/apple-touch-icon.png", "assets/favicon.ico", "assets/img/**/*.*", "assets/css/**/*.*", "assets/js/**/*.*", "assets/fonts/**/*.*"] %>,

      "config": {
        "site": <%= read 'config/site.json' %>
      }
    },
    <%= read 'pages/about.json' %>,
    <%= read 'pages/credits.json' %>,
    <%= read 'pages/imprint.json' %>,
    <%= read 'posts/IBM-Social-Business-Toolkit-Evaluation.json' %>
  ]
}
